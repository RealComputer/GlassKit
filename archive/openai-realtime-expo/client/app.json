{
  "expo": {
    "name": "client",
    "slug": "client",
    "scheme": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios"
    ],
    "ios": {
      "bundleIdentifier": "com.example.tash-2s.client"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "plugins": [
      "expo-router",
      "@config-plugins/react-native-webrtc",
      "expo-camera"
    ]
  }
}
